{
  "name": "@gpa-lab/guillotine",
  "version": "0.0.1",
  "description": "Plugin to accompany the GPA/LAB headless WordPress theme",
  "author": "U.S. Department of State, Bureau of Global Public Affairs Digital Lab <gpa-lab@america.gov>",
  "contributors": [
    {
      "name": "Marek Rewers"
    }
  ],
  "main": "index.js",
  "scripts": {
    "build": "npm run build:docs && npm run build:plugin",
    "build:docs": "wp-scripts build docs-hub/js/*.js --output-path=docs-hub/build",
    "build:plugin": "wp-scripts build admin/plugins/docs-sidebar.js --output-path=admin/build",
    "dev": "npm run dev:docs && npm run dev:plugin",
    "dev:docs": "wp-scripts start docs-hub/js/*.js --output-path=docs-hub/build",
    "dev:plugin": "wp-scripts start admin/plugins/docs-sidebar.js --output-path=admin/build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@wordpress/scripts": "^12.3.0",
    "prop-types": "^15.7.2"
  },
  "dependencies": {
    "@wordpress/components": "^11.0.0",
    "@wordpress/compose": "^3.21.0",
    "@wordpress/data": "^4.24.0",
    "@wordpress/edit-post": "^3.24.0",
    "@wordpress/element": "^2.18.0",
    "@wordpress/i18n": "^3.16.0",
    "@wordpress/plugins": "^2.22.0"
  }
}
