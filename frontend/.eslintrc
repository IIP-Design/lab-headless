{
  "ignorePatterns": [ ".next/", "node_modules/" ],
  "rules": {
    "node/no-unpublished-import": [
      "error", {
        "allowModules": ["prop-types"]
      }
    ],
    "node/no-unpublished-require": [
      "error", {
        "allowModules": ["@next/bundle-analyzer"]
      }
    ],
    "react/no-danger": "warn"
  }
}