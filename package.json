  "description": "Plugin to accompany the GPA/LAB headless WordPress theme",
  "author": "U.S. Department of State, Bureau of Global Public Affairs Digital Lab <gpa-lab@america.gov>",
  "main": "index.js",
  "scripts": {
    "build": "wp-scripts build admin/sidebars/docs-sidebar.js --output-path=admin/build",
    "dev": "wp-scripts start admin/sidebars/docs-sidebar.js --output-path=admin/build",
