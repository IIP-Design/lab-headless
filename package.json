    "url": "git+https://github.com/IIP-Design/lab-guillotine.git"
  },
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/IIP-Design/lab-guillotine/issues"
  },
  "homepage": "https://github.com/IIP-Design/lab-guillotine#readme",
  "devDependencies": {
    "@gpa-lab/eslint-config": "^1.3.1",
    "@wordpress/scripts": "^12.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.5.2"
  }
}
